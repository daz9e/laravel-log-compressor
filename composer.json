    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "daze",
            "email": "grigpavel21@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "laravel/framework": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Daz9e\\LaravelLogCompressor\\": "src/"
        }
    },
    "extra": {
